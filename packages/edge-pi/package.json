{
	"name": "edge-pi",
	"version": "0.2.0",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./node": {
			"types": "./dist/node.d.ts",
			"import": "./dist/node.js"
		},
		"./webcontainer": {
			"types": "./dist/webcontainer.d.ts",
			"import": "./dist/webcontainer.js"
		}
	},
	"browser": {
		"./dist/runtime/node-runtime.js": "./src/runtime/node-runtime.browser.ts",
		"./src/runtime/node-runtime.js": "./src/runtime/node-runtime.browser.ts",
		"./src/runtime/node-runtime.ts": "./src/runtime/node-runtime.browser.ts",
		"./dist/session/session-manager.js": "./src/session/session-manager.browser.ts",
		"./src/session/session-manager.js": "./src/session/session-manager.browser.ts",
		"./src/session/session-manager.ts": "./src/session/session-manager.browser.ts"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/marcusschiesser/edge-pi"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsgo -p tsconfig.build.json",
		"clean": "rm -rf dist",
		"test": "vitest --run"
	},
	"dependencies": {
		"ai": "^6.0.71",
		"diff": "^7.0.0",
		"file-type": "^20.5.0",
		"glob": "^11.0.3",
		"proper-lockfile": "^4.1.2",
		"zod": "^4.1.8"
	},
	"peerDependencies": {
		"@webcontainer/api": "^1.5.1"
	},
	"peerDependenciesMeta": {
		"@webcontainer/api": {
			"optional": true
		}
	},
	"devDependencies": {
		"@webcontainer/api": "^1.5.1",
		"@types/diff": "^7.0.2",
		"@types/proper-lockfile": "^4.1.4",
		"vitest": "^4.0.18"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
